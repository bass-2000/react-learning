{
  "presets": [
    [
      "@babel/env",
      {
        "corejs": 3,
        "useBuiltIns": "entry",
        "debug": true,
        "modules": false,
        "targets": [
          "last 2 chrome versions",
          "last 2 firefox versions"
        ]
      }
    ],
    [
      "@babel/react"
    ]
  ],
  "plugins": [
    "@babel/proposal-class-properties"
  ]
}